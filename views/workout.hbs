
<a class="btn btn-large" href="/exercises" >Add more Lifts</a>
<p>
{{#if message}}
{{message}}
{{/if}}
</p>





<section class="container" ng-app="workoutApp">


      <article class="row" id="articlechildren" ng-controller="workoutCtrl" >
        <div class="workOutCards" >
          <ul ng-sortable="workoutConfig" id="workoutList">
            <li ng-repeat="lift in workout|orderBy:'Position'" class="workoutModule workout-module col l6" id="//lift._id//">
              <div class="card">
                <div class="card-content">
                  <a href="#" ng-click="removeLift(lift)" data-lift="//lift._id//"><i class="small material-icons">delete</i></a>
                    <span class="card-title">
                    <!-- <input placeholder="Position is //lift.Position//"></input> -->
                    <strong>//lift.Name//</strong></span>
                    <p><em>//lift.Description//</em></p>
                    <!-- <div class="input-field"> -->
                    <label for="sets"> SETS:</label>
                    <input class="editable" ng-blur="addSets(sets,lift)" data-lift="//lift._id//" placeholder="//lift.sets//" type="text" name="sets" ng-model="sets">
                    <!-- </div> -->
                    <!-- <div class="input-field"> -->
                    <label for="reps"> REPS:</label>
                    <input class="editable" ng-blur="addReps(reps,lift)" data-lift="//lift._id//" placeholder="//lift.reps//" type="text" name="reps" ng-model="reps">
                    <!-- </div> -->
                    <!-- <div class="input-field"> -->
                    <label for="weight"> WEIGHT:</label>
                    <input class="editable" ng-blur="addWeight(weight,lift)" data-lift="//lift._id//" placeholder="//lift.weight//" type="text" name="weight" ng-model="weight">
                    <!-- </div> -->
                </div>
              </div>
            </li>
          </ul>
        </div>
      </article>

    </section>
